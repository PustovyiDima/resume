<section class="col-12 my-3">
    <ul class="breadcrumb">
        {{#each breadcrumb}}
        <li class="breadcrumb-item  mb-2">
            {{#if url}}
            <a href={{url}}> {{name}} </a>
            {{else}}
            {{name}}
            {{/if}}
        </li>
        {{/each}}
    </ul>
</section>